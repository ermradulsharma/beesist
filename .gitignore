# Beesist - .gitignore

# ----------------------------------------------------------------------
# Core Laravel
# ----------------------------------------------------------------------
/vendor
/node_modules
/public/hot
/public/storage
/public/build
/public/mix-manifest.json
/storage/*.key
.env
.env.*
!.env.example
.phpunit.result.cache

# ----------------------------------------------------------------------
# Modules & Plugins
# ----------------------------------------------------------------------
/Modules/*/assets
modules_statuses.json

# ----------------------------------------------------------------------
# Storage & Generated Files
# ----------------------------------------------------------------------
/storage/app/public/
/storage/framework/cache/data/
/storage/framework/views
/storage/framework/sessions
/storage/framework/testing
/storage/logs/*.log
/storage/debugbar

# ----------------------------------------------------------------------
# IDE & Tooling
# ----------------------------------------------------------------------
.idea/
.vscode/
.fleet/
.zed/
*.sublime-project
*.sublime-workspace
.phpstorm.meta.php
_ide_helper.php
_ide_helper_models.php
.php-cs-fixer.cache
.php_cs.cache

# ----------------------------------------------------------------------
# OS Specific
# ----------------------------------------------------------------------
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# ----------------------------------------------------------------------
# Build, Logs & temporary
# ----------------------------------------------------------------------
npm-debug.log
yarn-error.log
error_log
error.log
*.log
*.tmp
/coverage
coverage.xml

# ----------------------------------------------------------------------
# User Uploads
# ----------------------------------------------------------------------
/public/uploads/*
!/public/uploads/.gitkeep

# ----------------------------------------------------------------------
# Environment & Local Configurations
# ----------------------------------------------------------------------
Homestead.json
Homestead.yaml
auth.json
.fleet
.env.backup
output.txt
routes.json

# ----------------------------------------------------------------------
# Database / Backups
# ----------------------------------------------------------------------
*.sql
*.sqlite
