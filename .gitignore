# Beesist - .gitignore

# --- Core Laravel ---
/vendor
/node_modules
/public/storage
/public/hot
/public/mix-manifest.json
/storage/*.key
/storage/debugbar
.env
.env.backup
.env.production
.phpunit.result.cache
/coverage
coverage.xml

# --- Modules & Plugins ---
/Modules/*/assets
modules_statuses.json

# --- IDE Helper / Tooling ---
.phpstorm.meta.php
_ide_helper.php
_ide_helper_models.php
.php-cs-fixer.cache
.php_cs.cache
.vscode/
.idea/
*.sublime-project
*.sublime-workspace

# --- OS Specific ---
.DS_Store
Thumbs.db

# --- Build & Logs ---
npm-debug.log
yarn-error.log
error.log
/public/uploads/*
!/public/uploads/.gitkeep

# --- Docker & Sail ---
# docker/
# docker-compose.override.yml

# --- Local Temporary Files ---
output.txt
routes.json
Homestead.json
Homestead.yaml

# --- Miscellaneous ---
*.zip
/storage/app/public/
/storage/framework/cache/data/
/storage/framework/views/*.php
/storage/framework/sessions/
/storage/logs/*.log
